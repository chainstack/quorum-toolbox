{
    "useWhiteList": false,
    "jdbc": {
        "username": "cs",
        "url": "jdbc:h2:/run-quorum/tessera/db;MODE=Oracle;TRACE_LEVEL_SYSTEM_OUT=0",
        "autoCreateTables": true
    },
    "serverConfigs":[
        {
            "app":"Q2T",
            "enabled": true,
            "serverAddress":"unix:/run-quorum/tessera/tessera.ipc",
            "communicationType" : "REST"
        },
        {
            "app":"P2P",
            "enabled": true,
            "serverAddress":"${url}",
            "sslConfig": {
                "tls": "STRICT",
                "generateKeyStoreIfNotExisted": true,
                "serverKeyStore": "server-keystore",
                "serverKeyStorePassword": "",
                "serverTrustMode": "TOFU",
                "knownClientsFile": "knownClients",
                "clientKeyStore": "client-keystore",
                "clientKeyStorePassword": "",
                "clientTrustMode": "TOFU",
                "knownServersFile": "knownServers"
            },
            "communicationType" : "REST"
        }
    ],
    "keys": {
        "keyData": [
            {
                "privateKeyPath": "${privatekeys}",
                "publicKeyPath": "${publickeys}"
            }
        ]
    }
}
